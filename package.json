{
  "name": "banking-app",
  "version": "0.0.1",
  "description": "A modern banking application built with NestJS, TypeORM, and CQRS pattern",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create src/migrations/$npm_config_name",
    "db:migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate src/migrations/CreateUsersTable -d typeorm.config.js",
    "db:migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d typeorm.config.js",
    "db:migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -d typeorm.config.js",
    "db:migration:show": "ts-node ./node_modules/typeorm/cli.js migration:show -d typeorm.config.js",
    "db:schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop -d typeorm.config.js",
    "db:schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync -d typeorm.config.js",
    "db:seed": "ts-node src/database/seeds/seed.ts",
    "mcp:server": "ts-node src/mcp/mcp-server.ts",
    "mcp:build": "tsc src/mcp/mcp-server.ts --outDir dist/src/mcp --esModuleInterop --module commonjs --target es2020 --moduleResolution node"
  },
  "dependencies": {
    "@cfworker/json-schema": "^4.1.1",
    "@modelcontextprotocol/sdk": "^1.21.0",
    "@nestjs-mcp/server": "^0.3.0",
    "@nestjs/common": "^10.4.20",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@openai/codex": "^0.55.0",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "bcrypt": "^6.0.0",
    "bitbucket-mcp": "^5.0.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.26",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
